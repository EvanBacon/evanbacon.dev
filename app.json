{
  "expo": {
    "name": "Bacon",
    "slug": "bacon",
    "scheme": "bacon",
    "privacy": "public",
    "platforms": [
      "ios",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "default",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#000",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "newArchEnabled": true,
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "server"
    },
    "android": {
      "package": "evanbacon.dev"
    },
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "ios": {
      "bundleIdentifier": "evanbacon.dev",
      "supportsTablet": true,
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": true,
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "",
          "headOrigin": "https://evanbacon.dev",
          "asyncRoutes": true
        }
      ],
      "expo-quick-actions"
    ],
    "extra": {
      "eas": {
        "projectId": "d2118d4b-3005-4939-848c-3ba54b81e7fe"
      }
    },
    "owner": "bacon_staging"
  }
}
