{
  "expo": {
    "name": "Bacon Blog",
    "slug": "bacon",
    "scheme": "bacon",
    "privacy": "public",
    "platforms": ["web"],
    "version": "1.0.0",
    "orientation": "default",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#000",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "newArchEnabled": true,
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "server"
    },
    "android": {
      "package": "evanbacon.dev"
    },
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "ios": {
      "bundleIdentifier": "evanbacon.dev",
      "supportsTablet": false,
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": true,
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "",
          "headOrigin": "https://evanbacon.dev",
          "asyncRoutes": true
        }
      ],
      "expo-quick-actions"
    ],
    "extra": {
      "router": {
        "origin": "",
        "headOrigin": "https://evanbacon.dev",
        "asyncRoutes": true
      },
      "eas": {
        "projectId": "64b8e4a4-baf5-4160-90fe-ecfa7a32e6b8"
      }
    }
  }
}
