{
  "expo": {
    "name": "Bacon",
    "slug": "bacon",
    "scheme": "bacon",
    "privacy": "public",
    "platforms": [
      "web"
    ],
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "server"
    },
    "android": {
      "package": "evanbacon.dev"
    },
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "ios": {
      "bundleIdentifier": "evanbacon.dev",
      "supportsTablet": true
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://evanbacon.dev/",
          "asyncRoutes": {
            "web": true,
            "default": false
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d2118d4b-3005-4939-848c-3ba54b81e7fe"
      }
    },
    "owner": "bacon_staging"
  }
}
